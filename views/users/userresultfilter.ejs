        href="https://cdn.jsdelivr.net/gh/habibmhamadi/multi-select-tag/dist/css/multi-select-tag.css">
    <script src="https://cdn.jsdelivr.net/gh/habibmhamadi/multi-select-tag/dist/js/multi-select-tag.js"></script>
</head>

<body>
    <div class="container" style="margin-right: 6%;">
        <!-- <div style="padding-left: 20px; padding-top: 20px;" >
            <a href="/scholarships/filter">ย้อนกลับ</a>
        </div> -->
        <h2 style="text-align: center; padding-top: 25px;">ผลลัพธ์</h2>
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>ชื่อทุน</th>
                    <th>เวลาเปิด</th>
                    <th>เวลาปิด</th>
                    <th>ประเภททุน</th>
                    <th>มหาวิทยาลัย</th>
                    <th>คณะ</th>
                    <th><input type="checkbox" id="check_all"></th>
                </tr>
            </thead>
            <tbody>
                <% Object.values(scholarships).forEach(function(item){%>
                    <tr>
                        <td><a href="/users/this/<%= item._id%>">
                                <%= item.sname%>
                            </a></td>
                        <td>
                            <%= item.opendate%>
                        </td>
                        <td>
                            <%= item.closedate%>
                        </td>
                        <td>
                            <%= item.stype%>
                        </td>
                        <td>
                            <%= item.university%>
                        </td>
                        <td>
                            <%= item.sfaculty%>
                        </td>
                        <td>
                            <input type="checkbox" name="selecter_id" id="row-check" value="<%= item._id%>">
                        </td>
                    </tr>
                    <% }) %>
            </tbody>
        </table>
    </div>
</body>

</html>